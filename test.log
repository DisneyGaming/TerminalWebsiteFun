function openEmbed() {
    // Create a new div element
    var div = document.createElement('div');

    // Set the div's style so it appears on top of the current content
    div.style.position = 'fixed';
    div.style.top = '0';
    div.style.left = '0';
    div.style.width = '100%';
    div.style.height = '100%';
    div.style.backgroundColor = 'rgba(0, 0, 0, 0.5)';
    div.style.zIndex = '1000';

    // Create a new iframe element for the embed
    var iframe = document.createElement('iframe');

    // Set the iframe's source to the embed URL
    iframe.src = 'https://www.youtube.com/embed/dQw4w9WgXcQ';

    // Set the iframe's style so it appears in the center of the div
    iframe.style.position = 'absolute';
    iframe.style.top = '50%';
    iframe.style.left = '50%';
    iframe.style.width = '560px';
    iframe.style.height = '315px';
    iframe.style.transform = 'translate(-50%, -50%)';

    // Append the iframe to the div
    div.appendChild(iframe);

    // Append the div to the body of the document
    document.body.appendChild(div);
}

// Call the function
openEmbed();